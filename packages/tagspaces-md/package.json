{
  "name": "@tagspaces/tagspaces-md",
  "description": "MD editor used by the TagSpaces application and extensions",
  "version": "3.0.0",
  "author": "TagSpaces Authors <contact@tagspaces.org>",
  "license": "MIT",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/",
    "esm/",
    "src/",
    "LICENSE"
  ],
  "scripts": {
    "clean": "npx rimraf build lib esm lib-esm",
    "lib:cjs": "tsc -p tsconfig.json --target ES5 --module commonjs --outDir lib",
    "lib:esm": "tsc -p tsconfig.json --target ES5 --module ESNext --outDir esm",
    "copy-files": "npx shx cp src/style.module.css esm && npx shx cp src/style.module.css lib && npx shx cp src/milkdown.css esm && npx shx cp src/milkdown.css lib",
    "build": "npm run clean && npm run lib:cjs && npm run lib:esm && npm run copy-files",
    "build-webpack": "npm run clean && tsc && tsc -m es6 --outDir lib-esm && webpack --config webpack.config.js --mode production",
    "build-dev": "npm run clean && tsc && tsc -m es6 --outDir lib-esm && webpack --config webpack.config.js --mode development",
    "code-format": "npx prettier --write .",
    "npmpack": "npm run build && npm pack && tar -xvf tagspaces-tagspaces-md-3.0.0.tgz && npx shx cp -rL package/** ../../../tagspaces/node_modules/@tagspaces/tagspaces-md && rimraf package",
    "pkg-publish": "npm run build && npm publish --access public"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@milkdown/core": "^7.0.1",
    "@milkdown/ctx": "^7.0.1",
    "@milkdown/plugin-block": "7.1.1",
    "@milkdown/plugin-clipboard": "^7.1.1",
    "@milkdown/plugin-cursor": "7.1.1",
    "@milkdown/plugin-diagram": "^7.1.0",
    "@milkdown/plugin-emoji": "^7.1.1",
    "@milkdown/plugin-history": "^7.0.1",
    "@milkdown/plugin-listener": "^7.0.1",
    "@milkdown/plugin-math": "^7.1.0",
    "@milkdown/plugin-prism": "^7.0.1",
    "@milkdown/plugin-slash": "^7.1.0",
    "@milkdown/plugin-tooltip": "^7.1.0",
    "@milkdown/plugin-trailing": "^7.1.1",
    "@milkdown/plugin-upload": "^7.1.0",
    "@milkdown/preset-commonmark": "^7.0.1",
    "@milkdown/preset-gfm": "^7.0.1",
    "@milkdown/prose": "^7.0.1",
    "@milkdown/react": "^7.0.1",
    "@milkdown/theme-nord": "7.1.1",
    "@milkdown/transformer": "^7.0.1",
    "@milkdown/utils": "7.1.1",
    "@prosemirror-adapter/react": "^0.2.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.19.3",
    "@babel/preset-env": "^7.19.3",
    "@babel/preset-react": "^7.18.6",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "babel-loader": "^8.2.5",
    "css-loader": "^6.7.1",
    "prettier": "^2.7.1",
    "ts-loader": "^9.4.1",
    "typescript": "^4.8.4"
  },
  "peerDependencies": {
    "@emotion/react": "*",
    "@emotion/styled": "*",
    "@mui/icons-material": "*",
    "@mui/material": "*",
    "react": "*",
    "react-dom": "*"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
