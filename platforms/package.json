{
  "name": "@tagspaces/tagspaces-platforms",
  "description": "TagSpaces platforms module",
  "version": "1.3.45",
  "author": "TagSpaces Authors <contact@tagspaces.org>",
  "license": "MIT",
  "scripts": {
    "install": "node install/platformIO-new.js",
    "install-new": "npx @tagspaces/dynamic-packages-loading ./",
    "install-old": "node install/platformIO.js",
    "install-node": "npx cross-env PD_PLATFORM=node npm run install",
    "install-electron": "npx cross-env PD_PLATFORM=electron npm run install",
    "install-web": "npx cross-env PD_PLATFORM=web npm run install",
    "install-cordova": "npx cross-env PD_PLATFORM=cordova npm run install",
    "npmpack": "npm run install-electron && npm pack && tar -xvf tagspaces-tagspaces-platforms-1.3.27.tgz && npx shx cp -rL package/** ../../tagspaces2/node_modules/@tagspaces/tagspaces-platforms/ && npx rimraf package",
    "pkg-publish": "npm publish --access public"
  },
  "files": [
    "index.js",
    "misc.js",
    "paths.js",
    "utils-common.js",
    "AppConfig.js",
    "aws.js",
    "webdav.js",
    "indexer.js",
    "platform-io.js",
    "LICENSE",
    "install/**"
  ],
  "devDependencies": {
    "@tagspaces/dynamic-packages-loading": "^1.0.8"
  },
  "nodeDependencies": {
    "@tagspaces/tagspaces-common-node": "1.3.21",
    "@tagspaces/tagspaces-common-aws": "1.3.19",
    "@tagspaces/tagspaces-common-webdav": "1.0.18"
  },
  "webDependencies": {
    "@tagspaces/tagspaces-common-aws": "1.3.19",
    "@tagspaces/tagspaces-common-webdav": "1.0.18"
  },
  "cordovaDependencies": {
    "@tagspaces/tagspaces-common-cordova": "1.3.14",
    "@tagspaces/tagspaces-common-aws": "1.3.19",
    "@tagspaces/tagspaces-common-webdav": "1.0.18"
  },
  "electronDependencies": {
    "@tagspaces/tagspaces-common-node": "1.3.21",
    "@tagspaces/tagspaces-common-electron": "1.0.8",
    "@tagspaces/tagspaces-common-aws": "1.3.19",
    "@tagspaces/tagspaces-common-webdav": "1.0.18"
  }
}
